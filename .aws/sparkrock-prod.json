{
  "containerDefinitions": [
    {
      "name": "sparkrock",
      "image": "ACCOUNT_ID.dkr.ecr.us-west-1.amazonaws.com/sparkrock/sparkrock_prod:latest",
      "cpu": 0,
      "portMappings": [
        {
          "name": "3000",
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "secrets": [
        {
          "name": "DB_HOST",
          "valueFrom": "arn:aws:ssm:us-west-1:ACCOUNT_ID:parameter/DB_HOST_PROD"
        },
        {
          "name": "DB_PORT",
          "valueFrom": "arn:aws:ssm:us-west-1:ACCOUNT_ID:parameter/DB_PORT_PROD"
        },
        {
          "name": "DB_NAME",
          "valueFrom": "arn:aws:ssm:us-west-1:ACCOUNT_ID:parameter/DB_NAME_PROD"
        },
        {
          "name": "DB_PASSWORD",
          "valueFrom": "arn:aws:secretsmanager:us-west-1:ACCOUNT_ID:secret:prod/sparkrock-y5Gnrx:password::"
        },
        {
          "name": "DB_USERNAME",
          "valueFrom": "arn:aws:secretsmanager:us-west-1:ACCOUNT_ID:secret:prod/sparkrock-y5Gnrx:username::"
        },
        {
          "name": "CORS_ORIGIN",
          "valueFrom": "arn:aws:ssm:us-west-1:ACCOUNT_ID:parameter/ALLOWED_ORIGIN_PROD"
        },
        {
          "name": "JWT_SECRET",
          "valueFrom": "arn:aws:ssm:us-west-1:ACCOUNT_ID:parameter/JWT_SECRET_PROD"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/sparkrock-prod",
          "awslogs-region": "us-west-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://localhost:3000/health || exit 1"
        ],
        "interval": 20,
        "timeout": 10,
        "retries": 3,
        "startPeriod": 30
      },
      "systemControls": []
    }
  ],
  "family": "sparkrock-prod",
  "executionRoleArn": "arn:aws:iam::ACCOUNT_ID:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::ACCOUNT_ID:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}
